{
  "name": "ECorpApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 15 Pro Max\"",
    "ios:device": "react-native run-ios --device \"Bryan's iPhone\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "start": "react-native start",
    "stop": "lsof -ti :8081 | xargs kill -9 2>/dev/null || true",
    "stop:ios": "npm run stop && xcrun simctl shutdown all",
    "stop:android": "npm run stop && adb shell am force-stop com.bryan.ecorpapp 2>/dev/null || true",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "validate": "npm run type-check && npm run lint && npm run test",
    "prepare-release": "./scripts/prepare-release.sh",
    "archive": "./scripts/archive.sh",
    "ios:release": "react-native run-ios --configuration Release",
    "screenshots": "mkdir -p screenshots/iphone-6.9 screenshots/iphone-6.7 screenshots/iphone-6.5 && echo 'Screenshot folders created! Run the app and take screenshots.'"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.82.0",
    "@react-navigation/bottom-tabs": "^7.4.8",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.27",
    "@shopify/react-native-skia": "^2.3.0",
    "react": "19.1.1",
    "react-native": "0.82.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-vision-camera": "^4.7.2",
    "victory-native": "^41.20.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@react-native-community/cli": "^20.0.2",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.0",
    "@react-native/eslint-config": "0.82.0",
    "@react-native/metro-config": "0.82.0",
    "@react-native/typescript-config": "0.82.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^16.2.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=-1",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
